EAAA,KAAA,IAAA,CAAA,MAPA;AAQA,QAAA,kBAAA,EAAA,KAAA,IAAA,CAAA,kBARA;AASA,QAAA,MAAA,EAAA,KAAA,IAAA,CAAA;AATA,OAAA;AAWA,MAAA,KAAA,CAAA;AACA,QAAA,MAAA,EAAA,MADA;AAEA,QAAA,GAAA,EAAA,KAAA,IAAA,GAAA,gBAFA;AAGA,QAAA,IAAA,EAAA,KAAA,IAHA;AAIA,QAAA,MAAA,EAAA;AAAA,UAAA,OAAA,EAAA;AAAA,4BAAA;AAAA;AAAA;AAJA,OAAA,CAAA,CAMA,IANA,CAMA,UAAA,QAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,yBAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,SAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,QAAA,MAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAXA,WAYA,UAAA,KAAA,EAAA;AACA,QAAA,MAAA,CAAA,MAAA,GAAA,UAAA,KAAA;AACA,QAAA,MAAA,CAAA,KAAA,GAAA,OAAA;AACA,QAAA,MAAA,CAAA,QAAA,GAAA,IAAA;AACA,OAhBA;AAkBA;AAtDA;AA/DA,CAAA","sourcesContent":["<template>\n  <div>\n    <v-snackbar multi-line :color=\"color\" top right v-model=\"snackbar\">\n      {{ status }}\n      <v-btn\n        dark\n        text\n        @click=\"snackbar = false\"\n      >\n        Close\n      </v-btn>\n    </v-snackbar>\n    <v-app-bar id=\"home-app-bar\" app color=\"white\" elevation=\"1\" height=\"80\">\n      <base-img\n        :src=\"require('@/assets/logo_da.png')\"\n        class=\"mr-3\"\n        contain\n        max-width=\"120\"\n        width=\"100%\"\n      />\n      <v-spacer />\n      <div>\n        <v-tabs\n          class=\"hidden-sm-and-down\"\n          optional\n        >\n          <v-tab \n            to=\"/\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Belajar\n          </v-tab>\n          <!-- <v-tab \n            to=\"/about\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Kelas Yang Diikuti\n          </v-tab> -->\n          <v-tab \n            to=\"/profile\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Profile\n          </v-tab>\n          <v-tab \n            @click=\"login()\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Masuk\n          </v-tab>\n          <v-tab \n            @click=\"daftar()\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Daftar\n          </v-tab>\n        </v-tabs>\n      </div>\n      <v-app-bar-nav-icon\n        class=\"hidden-md-and-up\"\n        @click=\"drawer = !drawer\"\n      />\n    </v-app-bar>\n    <v-navigation-drawer\n      bottom\n      color=\"transparent\"\n      fixed\n      v-model=\"drawer\"\n      height=\"auto\"\n      overlay-color=\"secondary\"\n      overlay-opacity=\".8\"\n      temporary\n      v-bind=\"$attrs\"\n      v-on=\"$listeners\"\n    >\n      <v-list color=\"white\" shaped>\n        <v-list-item to=\"/\" color=\"primary\">\n          <v-list-item-content>\n            <v-list-item-title v-text=\"'Beranda'\" />\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item to=\"/profile\" color=\"primary\">\n          <v-list-item-content>\n            <v-list-item-title v-text=\"'Profile'\" />\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item @click=\"login()\" color=\"primary\">\n          <v-list-item-content>\n            <v-list-item-title v-text=\"'Login'\" />\n          </v-list-item-content>\n        </v-list-item>\n        <v-list-item @click=\"daftar()\" color=\"primary\">\n          <v-list-item-content>\n            <v-list-item-title v-text=\"'Daftar'\" />\n          </v-list-item-content>\n        </v-list-item>\n      </v-list>\n    </v-navigation-drawer>\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Masuk</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Email*\" hint=\"email yang telah didaftarkan\"\n                  persistent-hint\n                  required></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Password*\" type=\"password\" required></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Tutup</v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Masuk</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    <v-dialog v-model=\"dialog2\" persistent max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Daftar</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row no-gutters>\n              <v-col cols=\"12\" style=\"margin-bottom:10px;\">\n                <v-text-field v-model=\"auth.full_name\" label=\"Nama*\" required hint=\"nama lengkap akan digunakan untuk sertifikat kelulusan\"\n                  persistent-hint></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field v-model=\"auth.email\" label=\"Email*\" \n                  required></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field v-model=\"auth.password\" label=\"Password*\" type=\"password\" required></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field v-model=\"auth.location\" label=\"Alamat Lengkap\"></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field v-model=\"auth.phone\" label=\"Nomer Hp\"></v-text-field>\n              </v-col>\n              <v-row style=\"padding-top:0px;\">\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\" style=\"padding-top:0px;\">\n                  <v-text-field v-model=\"auth.school\" label=\"Sekolah\" required hint=\"tuliskan lulus jika sudah tidak bersekolah\"\n                  persistent-hint></v-text-field>\n                </v-col>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\" style=\"padding-top:0px;\">\n                  <v-dialog\n                    ref=\"dialog\"\n                    v-model=\"modal\"\n                    :return-value.sync=\"date\"\n                    persistent\n                    width=\"290px\"\n                  >\n                    <template v-slot:activator=\"{ on }\">\n                      <v-text-field\n                        v-model=\"date\"\n                        label=\"Picker in dialog\"\n                        prepend-icon=\"event\"\n                        readonly\n                        v-on=\"on\"\n                      ></v-text-field>\n                    </template>\n                    <v-date-picker v-model=\"date\" scrollable>\n                      <v-spacer></v-spacer>\n                      <v-btn text color=\"primary\" @click=\"modal = false\">Cancel</v-btn>\n                      <v-btn text color=\"primary\" @click=\"$refs.dialog.save(date)\">OK</v-btn>\n                    </v-date-picker>\n                  </v-dialog>\n                </v-col>\n               </v-row>\n              <v-row>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\">\n                  <v-select\n                    item-text=\"type_of_disability\"\n                    item-value=\"id\"\n                    v-model=\"auth.type_of_disability\"\n                    :items=\"jenis_disabilitas\"\n                    label=\"Jenis Disabilitas\"\n                  ></v-select>\n                </v-col>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\">\n                  <v-select\n                    item-text=\"gender\"\n                    item-value=\"id\"\n                    v-model=\"auth.gender\"\n                    :items=\"jenis_kelamin\"\n                    label=\"Jenis Kelamin\"\n                  ></v-select>\n                </v-col>\n              </v-row>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog2 = false\">Tutup</v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"save()\" >Daftar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\n  export default {\n    name: 'HomeAppBar',\n\n    data: () => ({\n      status: '',\n      color: '',\n      snackbar: false,\n      date: new Date().toISOString().substr(0, 10),\n      menu: false,\n      modal: false,\n      menu2: false,\n      host: 'https://api.diffableacademy.com/index.php',\n      fromDateMenu: false,\n      fromDateVal: null,\n      minDate: \"2020-07-04\",\n      maxDate: \"2021-08-30\",\n      items: [\n        'Tuna Netra',\n        'Tuna Rungu',\n        'Tuna Wicara',\n        'Tuna Daksa,',\n        'Tuna Laras',\n        'Tuna Grahita',\n        'Tuna Ganda',\n        'Lain-lain'\n      ],\n      items2: [\n        'Pria',\n        'Wanita'\n      ],\n      dialog: false,\n      dialog2: false,\n      drawer: null,\n      items: [\n        'Beranda',\n        'Profile',\n        'Masuk',\n        'Daftar',\n      ],\n      jenis_kelamin: {},\n      jenis_disabilitas: {},\n      auth: {\n        email: '',\n        password: '',\n        full_name: '',\n        date_of_birth: '',\n        phone: '',\n        location: '',\n        school: '',\n        type_of_disability: '',\n        gender: ''\n      }\n    }),\n    computed: {\n      fromDateDisp() {\n        return this.fromDateVal;\n      }\n    },\n    mounted() {\n      // console.log('asdas')\n      this.getJenisKelamin()\n      this.getJenisDisabilitas()\n    },\n    methods: {\n      getJenisKelamin() {\n        axios.get(this.host + '/auth/gender').then(res => {\n          this.jenis_kelamin = res.data\n          console.log(this.jenis_kelamin)\n        }).catch ((err) => {\n          console.log(err);\n        })\n      },\n      getJenisDisabilitas() {\n        axios.get(this.host + '/auth/type_of_disability').then(res => {\n          this.jenis_disabilitas = res.data\n          console.log(this.jenis_disabilitas)\n        }).catch ((err) => {\n          console.log(err);\n        })\n      },\n      login() {\n        this.dialog = true\n      },\n      daftar() {\n        this.dialog2 = true\n      },\n      save() {\n        this.auth.date_of_birth = this.date\n        let rawData = {\n          email: this.auth.email,\n          password: this.auth.password,\n          full_name: this.auth.full_name,\n          date_of_birth: this.auth.date_of_birth,\n          phone: this.auth.phone,\n          location: this.auth.location,\n          school: this.auth.school,\n          type_of_disability: this.auth.type_of_disability,\n          gender: this.auth.gender\n        }\n        axios({\n          method: 'post',\n          url: this.host + '/auth/register',\n          data: this.auth,\n          config: { headers: { 'Content-Type': 'multipart/form-data' }}\n        })\n        .then(response => {\n          this.status = 'Data berhasil tersimpan'\n          this.color = 'success'\n          this.snackbar = true\n          this.dialog2 = false\n        })\n        .catch(error => {\n          this.status = 'Error' + error\n          this.color = 'error'\n          this.snackbar = true\n        })\n\n      }\n    }\n  }\n</script>\n\n<style lang=\"sass\">\n  #home-app-bar\n    .v-tabs-slider\n      max-width: 24px\n      margin: 0 auto\n\n    .v-tab\n      &::before\n        display: none\n</style>\n"],"sourceRoot":"src/layouts/home"}]}