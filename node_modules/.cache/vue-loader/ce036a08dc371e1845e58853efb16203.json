    contain\n        max-width=\"150\"\n        width=\"100%\"\n      />\n      <v-spacer />\n\n      <div>\n        <v-tabs\n          class=\"hidden-sm-and-down\"\n          optional\n        >\n          <v-tab \n            to=\"/\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Belajar\n          </v-tab>\n          <!-- <v-tab \n            to=\"/about\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            About\n          </v-tab> -->\n          <!-- <v-tab \n            to=\"/contact\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Contact\n          </v-tab> -->\n          <v-tab \n            to=\"/about\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Kelas Yang Diikuti\n          </v-tab>\n          <v-tab \n            @click=\"login()\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Masuk\n          </v-tab>\n          <v-tab \n            @click=\"daftar()\"\n            :ripple=\"false\"\n            active-class=\"text--primary\"\n            class=\"font-weight-bold\"\n            min-width=\"96\"\n            text>\n            Daftar\n          </v-tab>\n        </v-tabs>\n      </div>\n\n      <v-app-bar-nav-icon\n        class=\"hidden-md-and-up\"\n        @click=\"drawer = !drawer\"\n      />\n    </v-app-bar>\n    <home-drawer\n      v-model=\"drawer\"\n      :items=\"items\"\n    />\n    <v-dialog v-model=\"dialog\" persistent max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Masuk</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Email*\" hint=\"email yang telah didaftarkan\"\n                  persistent-hint\n                  required></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Password*\" type=\"password\" required></v-text-field>\n              </v-col>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Tutup</v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog = false\">Masuk</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n    <v-dialog v-model=\"dialog2\" persistent max-width=\"600px\">\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Daftar</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row no-gutters>\n              <v-col cols=\"12\" style=\"margin-bottom:10px;\">\n                <v-text-field label=\"Nama*\" required hint=\"nama lengkap akan digunakan untuk sertifikat kelulusan\"\n                  persistent-hint></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Email*\" \n                  required></v-text-field>\n              </v-col>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Password*\" type=\"password\" required></v-text-field>\n              </v-col>\n              <v-row style=\"padding-top:0px;\">\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\" style=\"padding-top:0px;\">\n                  <v-text-field label=\"Sekolah\" required hint=\"tuliskan lulus jika sudah tidak bersekolah\"\n                  persistent-hint></v-text-field>\n                </v-col>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\" style=\"padding-top:0px;\">\n                  <v-menu\n                    v-model=\"fromDateMenu\"\n                    :close-on-content-click=\"false\"\n                    :nudge-right=\"40\"\n                    lazy\n                    transition=\"scale-transition\"\n                    offset-y\n                    full-width\n                    max-width=\"290px\"\n                    min-width=\"290px\"\n                  >\n                    <template v-slot:activator=\"{ on }\">\n                      <v-text-field\n                        label=\"Tanggal Lahir\"\n                        prepend-icon=\"event\"\n                        readonly\n                        :value=\"fromDateDisp\"\n                        v-on=\"on\"\n                      ></v-text-field>\n                    </template>\n                    <v-date-picker\n                      locale=\"en-in\"\n                      :min=\"minDate\"\n                      :max=\"maxDate\"\n                      v-model=\"fromDateVal\"\n                      no-title\n                      @input=\"fromDateMenu = false\"\n                    ></v-date-picker>\n                  </v-menu>\n                </v-col>\n               </v-row>\n              <v-col cols=\"12\">\n                <v-text-field label=\"Alamat Lengkap\"></v-text-field>\n              </v-col>\n              <v-row>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\">\n                  <v-select\n                    :items=\"items\"\n                    label=\"Jenis Disabilitas\"\n                  ></v-select>\n                </v-col>\n                <v-col class=\"d-flex\" cols=\"6\" sm=\"6\">\n                  <v-select\n                    :items=\"items2\"\n                    label=\"Jenis Kelamin\"\n                  ></v-select>\n                </v-col>\n              </v-row>\n            </v-row>\n          </v-container>\n        </v-card-text>\n        <v-card-actions>\n          <v-spacer></v-spacer>\n          <v-btn color=\"blue darken-1\" text @click=\"dialog2 = false\">Tutup</v-btn>\n          <v-btn color=\"blue darken-1\" text @click=\"save()\" >Daftar</v-btn>\n        </v-card-actions>\n      </v-card>\n    </v-dialog>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\n  export default {\n    name: 'HomeAppBar',\n\n    components: {\n      HomeDrawer: () => import('./Drawer'),\n    },\n\n    data: () => ({\n      fromDateMenu: false,\n        fromDateVal: null,\n\n        minDate: \"2019-07-04\",\n        maxDate: \"2019-08-30\",\n      items: [\n        'Tuna Netra',\n        'Tuna Rungu',\n        'Tuna Wicara',\n        'Tuna Daksa,',\n        'Tuna Laras',\n        'Tuna Grahita',\n        'Tuna Ganda',\n        'Lain-lain'\n      ],\n      items2: [\n        'Pria',\n        'Wanita'\n      ],\n      dialog: false,\n      dialog2: false,\n      drawer: null,\n      items: [\n        'Home',\n        'About',\n        'Contact',\n        'Pro',\n      ],\n    }),\n    computed: {\n      fromDateDisp() {\n        return this.fromDateVal;\n      }\n    },\n    mounted() {\n      this.load()\n    },\n    methods: {\n      load(){\n        axios.get('http://dummy.restapiexample.com/api/v1/employees').then(res => {\n          console.log(res.data.data)\n        }).catch ((err) => {\n          console.log(err);\n        })\n      },\n      login() {\n        this.dialog = true\n      },\n      daftar() {\n        this.dialog2 = true\n      },\n      save() {\n        this.dialog2 = false\n      }\n    }\n  }\n</script>\n\n<style lang=\"sass\">\n  #home-app-bar\n    .v-tabs-slider\n      max-width: 24px\n      margin: 0 auto\n\n    .v-tab\n      &::before\n        display: none\n</style>\n"]}]}